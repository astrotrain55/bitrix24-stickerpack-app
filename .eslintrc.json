{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/vue3-essential"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["*.js"]
    }],
    "camelcase": ["error", {
      "ignoreDestructuring": true
    }]
  }
}
